
public class Solution {

	public void moveZeroes(int[] arr) {
		
		if(arr == null) {
			throw new IllegalArgumentException("Null Parameter Value");
		}
		
		int lastIndex = 0;
		
		for(int i = 0; i < arr.length; ++i) {
			
			if(arr[i] != 0) {
				
				arr[lastIndex++] = arr[i];
	
				// swap(arr[lastIndex++] = arr[i];
				
			}
			
		}
		
		
		for(int i = lastIndex; i < arr.length; ++i) {
			arr[i] = 0;
		}
		
				
	}
	
	
}
